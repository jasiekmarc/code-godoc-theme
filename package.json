{
    "name": "godoc-vscode-theme",
    "displayName": "Godoc Theme",
    "description": "Minimalistic themes for VS Code based on pkg.go.dev.",
    "version": "0.1",
    "publisher": "jasiekmarc",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/jasiekmarc/godoc-vscode-theme"
    },
    "bugs": {
      "url": "https://github.com/jasiekmarc/godoc-vscode-theme/issues"
    },
    "engines": {
      "vscode": "^1.43.0"
    },
    "categories": [
      "Themes"
    ],
    "keywords": [
      "theme",
      "light",
      "dark"
    ],
    "contributes": {
      "themes": [
        {
          "label": "Go Doc Light",
          "uiTheme": "vs",
          "path": "./themes/light.json"
        },
        {
          "label": "Go Doc Dark",
          "uiTheme": "vs-dark",
          "path": "./themes/dark.json"
        }
      ]
    },
    "devDependencies": {
      "vsce": "^1.100.1"
    },
    "scripts": {
      "build": "node src/index.js && mkdir -p build"
    }
    
  }